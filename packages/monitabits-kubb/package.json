{
	"private": true,
	"name": "@repo/monitabits-kubb",
	"type": "module",
	"version": "0.1.0",
	"description": "Kubb-generated API client (types, hooks, zod schemas)",
	"files": [
		"src/**"
	],
	"main": "./src/index.ts",
	"exports": {
		".": "./src/index.ts",
		"./mutator.client": "./src/mutator.client.ts",
		"./mutator.server": "./src/mutator.server.ts",
		"./hooks": "./src/gen/hooks/index.ts",
		"./server": "./src/gen/server/index.ts",
		"./types": "./src/gen/types/index.ts",
		"./zod": "./src/gen/zod/index.ts"
	},
	"scripts": {
		"check": "biome check .",
		"check:fix": "biome check . --write",
		"check:types": "tsc --noEmit",
		"kubb": "kubb && bun run check:fix"
	},
	"dependencies": {
		"swr": "2.3.7",
		"zod": "4.1.13"
	},
	"devDependencies": {
		"@kubb/cli": "4.8.1",
		"@kubb/core": "4.8.1",
		"@kubb/plugin-oas": "4.8.1",
		"@kubb/plugin-swr": "4.8.1",
		"@kubb/plugin-ts": "4.8.1",
		"@kubb/plugin-zod": "4.8.1",
		"@repo/test-preset": "workspace:*",
		"@repo/typescript-config": "workspace:*"
	},
	"peerDependencies": {
		"zod": "4.1.13"
	}
}
